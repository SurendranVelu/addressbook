pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compiling the code'
            }
        }
        stage('Unit Test') {
            steps {
                echo 'Testing the code'
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging the code'
            }
        }
    }
}
